@model GarageII_TheParking.Models.ViewModels.GarageViewModel
@{
    ViewBag.Title = Model.Garage.Name;



}

<h2>@ViewBag.Title</h2>


<p>
    @Html.ActionLink("Park", "Park")
</p>
<table class="table">
    @{ 
        var exampleVehicle = new GarageII_TheParking.Models.Vehicle();
    }
    <tr>
        <th>
            @Html.DisplayNameFor(model => exampleVehicle.Color)
        </th>
        <th>
            @Html.DisplayNameFor(model => exampleVehicle.RegistrationNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => exampleVehicle.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => exampleVehicle.PersonWhoParkedVechicle)
        </th>
        <th>
   
        </th>
    </tr>

@foreach (var item in Model.Vehicles) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Color)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegistrationNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Type.Name)
        </td>
        <td>

            @Html.DisplayFor(modelItem => $"{item.PersonWhoParkedVechicle.Name} {item.PersonWhoParkedVechicle.LastName}")
            
        </td>
        <td>
            @Html.ActionLink("Collect", "Collect", new { id = item.Id})
        </td>
        <td>
            @Html.ActionLink("Details",  "Details", new { id = item.Id })
        </td>
    </tr>
}

</table>
